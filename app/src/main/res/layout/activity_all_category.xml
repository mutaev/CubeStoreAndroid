<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:background="@drawable/back"
    android:layout_height="match_parent"
    tools:context=".AllCategory">


  <TextView
      android:id="@+id/textView12"
      android:layout_width="166dp"
      android:layout_height="47dp"
      android:fontFamily="@font/coiny"
      android:text="Cube 3^3"
      android:textColor="#ECE6E6"
      android:textSize="34sp"
      app:layout_constraintEnd_toEndOf="@+id/cube3"
      app:layout_constraintStart_toStartOf="@+id/cube3"
      app:layout_constraintTop_toBottomOf="@+id/cube3" />

  <TextView
      android:id="@+id/textView13"
      android:layout_width="166dp"
      android:layout_height="47dp"
      android:fontFamily="@font/coiny"
      android:text="Cube 4^4"
      android:textColor="#ECE6E6"
      android:textSize="34sp"
      app:layout_constraintEnd_toEndOf="@+id/cube4"
      app:layout_constraintHorizontal_bias="0.0"
      app:layout_constraintStart_toStartOf="@+id/cube4"
      app:layout_constraintTop_toBottomOf="@+id/cube4" />

  <ImageView
      android:id="@+id/imageView4"
      android:layout_width="35dp"
      android:layout_height="35dp"
      android:layout_marginEnd="16dp"
      app:layout_constraintBottom_toBottomOf="@+id/textView3"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintTop_toTopOf="@+id/textView3"
      app:srcCompat="@drawable/shoppingcart" />

  <TextView
      android:id="@+id/textView3"
      android:layout_width="wrap_content"
      android:layout_height="27dp"
      android:layout_marginTop="16dp"
      android:layout_marginEnd="160dp"
      android:fontFamily="@font/b612_mono_bold"
      android:text="КАТАЛОГ"
      android:textColor="#ECE6E6"
      android:textSize="20sp"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintTop_toTopOf="parent" />

  <ImageView
      android:id="@+id/back"
      android:layout_width="37dp"
      android:layout_height="38dp"
      android:layout_marginStart="16dp"
      android:layout_marginTop="16dp"
      app:layout_constraintBottom_toBottomOf="@+id/textView3"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:srcCompat="@drawable/nazadn" />

  <ImageView
      android:id="@+id/cube3"
      android:layout_width="170dp"
      android:layout_height="170dp"
      android:layout_marginEnd="24dp"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintTop_toTopOf="@+id/textView9"
      app:srcCompat="@drawable/catcube22" />

  <ImageView
      android:id="@+id/cube2"
      android:layout_width="170dp"
      android:layout_height="170dp"
      android:layout_marginStart="16dp"
      android:layout_marginTop="60dp"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:srcCompat="@drawable/cube222" />

  <ImageView
      android:id="@+id/cube4"
      android:layout_width="170dp"
      android:layout_height="170dp"
      android:layout_marginStart="24dp"
      android:layout_marginTop="56dp"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="@+id/textView12"
      app:srcCompat="@drawable/cube444" />

  <TextView
      android:id="@+id/textView9"
      android:layout_width="166dp"
      android:layout_height="47dp"
      android:layout_marginTop="4dp"
      android:fontFamily="@font/coiny"
      android:text="Cube 2^2"
      android:textColor="#ECE6E6"
      android:textSize="34sp"
      app:layout_constraintEnd_toEndOf="@+id/cube2"
      app:layout_constraintStart_toStartOf="@+id/cube2"
      app:layout_constraintTop_toBottomOf="@+id/cube2" />

</androidx.constraintlayout.widget.ConstraintLayout>